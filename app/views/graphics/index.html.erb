<p id="notice"><%= notice %></p>

<h1>Graphics</h1>

<table>
  <thead>
    <tr>
      <th>Analytics dashboard</th>
      <th>Graphics property</th>
      <th>Token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graphics.each do |graphic| %>
      <tr>
        <td><%= graphic.token %></td>
        <td><%= link_to 'Show', project_type_dashboard_graphic_path(@project_type, @dashboard, graphic) %></td>
        <td><%= link_to 'Edit', edit_project_type_dashboard_graphic_path(@project_type, @dashboard,graphic) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Graphic', new_project_type_dashboard_graphic_path %>
