<%- model_class = AnalyticsDashboard -%>
<p id="notice"><%= notice %></p>

<h1>Analytics Dashboards</h1>

<table class="table">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:title) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analytics_dashboards.each do |analytics_dashboard| %>
      <%= render "row", analytics_dashboard: analytics_dashboard %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analytics Dashboard', new_project_type_dashboard_analytics_dashboard_path(@project_type, @dashboard), remote: true %>
