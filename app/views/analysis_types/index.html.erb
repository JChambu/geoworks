<p id="notice"><%= notice %></p>

<h1>Analysis Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_types.each do |analysis_type| %>
      <tr>
        <td><%= analysis_type.name %></td>
        <td><%= analysis_type.description %></td>
        <td><%= link_to 'Show', analysis_type %></td>
        <td><%= link_to 'Edit', edit_analysis_type_path(analysis_type) %></td>
        <td><%= link_to 'Destroy', analysis_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis Type', new_analysis_type_path %>
