<p id="notice"><%= notice %></p>
<h1>Projects</h1>
<div>
</div>
<table class="table table-stripe">
  <thead>
    <tr>
      <% @fields.each do |field|  %>
        <th><%= order_link Project, field.name%></th>        
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project | %>
      <tr>
        <% @fields.each do |field|  %>
          <td>
              <%= project.properties[field.key] if !project.properties[field.key].blank?%>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
<br>


